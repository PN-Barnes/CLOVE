<div>
  <form>
    <div class='form-group row'>
      <label for='zipCode' class='col-sm-2 col-form-label'><b>Zip Code</b></label>
      <div class='col-sm-10'>
        <input
          type='text'
          class='form-control'
          id='zipCode'
          placeholder='Zip Code'
        />
      </div>
    </div>
    <div class='form-group row'>
      <label
        for='usernameInput'
        class='col-sm-2 col-form-label'
      ><b>Username</b></label>
      <div class='col-sm-10'>
        <input
          type='text'
          class='form-control'
          id='usernameInput'
          placeholder='Username'
        />
      </div>
    </div>
    <div class='form-group row'>
      <label for='productInput' class='col-sm-2 col-form-label'><b>Product</b></label>
      <div class='col-sm-10'>
        <input
          type='text'
          class='form-control'
          id='productInput'
          placeholder='Product name'
        />
      </div>
    </div>
    <div class='form-group row'>
      <div class='col-sm-10'>
        <button type='submit' class='btn'>Search</button>
      </div>
    </div>
  </form>
</div>

