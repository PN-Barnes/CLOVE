<div>
  <form>
    <div class='form-group row'>
      <label for='zipCode' class='col-sm-2 col-form-label'>Zip Code</label>
      <div class='col-sm-10'>
        <input
          type='text'
          class='form-control'
          id='zipCode'
          placeholder='Zip Code'
        />
      </div>
    </div>
    <div class='form-group row'>
      <label
        for='usernameInput'
        class='col-sm-2 col-form-label'
      >Username</label>
      <div class='col-sm-10'>
        <input
          type='text'
          class='form-control'
          id='usernameInput'
          placeholder='Username'
        />
      </div>
    </div>
    <div class='form-group row'>
      <label for='productInput' class='col-sm-2 col-form-label'>Product</label>
      <div class='col-sm-10'>
        <input
          type='text'
          class='form-control'
          id='productInput'
          placeholder='Product name'
        />
      </div>
    </div>
    <div class='form-group row'>
      <div class='col-sm-10'>
        <button type='submit' id="searchButton" class='btn btn-primary'>Search</button>
      </div>
    </div>
  </form>
</div>

{{#if searchBool}}
{{#each searchResult}}
{{> searchListingCard}}
{{/each}}
{{/if}}

<script src='./js/getSearch.js'></script>
